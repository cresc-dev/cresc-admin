{
  "compilerOptions": {
    "outDir": "dist",
    "module": "preserve",
    "moduleResolution": "bundler",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "strict": true,
    "resolveJsonModule": true,
    "baseUrl": "./src",
    "skipLibCheck": true,
    "paths": {
      "@/*": ["*"]
    }
  }
}
